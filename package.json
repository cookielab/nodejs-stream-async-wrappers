{
  "name": "@cookielab.io/stream-async-wrappers",
  "version": "0.2.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cookielab/nodejs-stream-async-wrappers.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "build": "bin/build",
    "watch": "bin/watch",
    "lint": "bin/lint || true",
    "flow": "bin/flow || true",
    "test": "bin/jest",
    "check-all": "bin/check-all || true",
    "prepare": "bin/build"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "dependencies": {
    "semaphore-async-await": "^1.5.1"
  },
  "devDependencies": {
    "@cookielab.io/eslint-config-server": "0.3.0",
    "@cookielab.io/eslint-plugin-custom-rules": "0.1.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-preset-node8": "1.2.0",
    "eslint": "5.10.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-flowtype": "3.2.0",
    "eslint-plugin-import": "2.14.0",
    "flow-bin": "0.87.0",
    "flow-copy-source": "2.0.2",
    "jest": "23.6.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/src",
      "<rootDir>/tests"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/tests/bootstrap.js"
    ],
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.{js,flow}"
    ]
  }
}
